#Faça um programa que determine se um número é divisível por 3 e por 5:

#Lógica

funcao NumerosDivisiveis(numero):

 se numero % 3 == 0 & numero % 5 == 0
  imprime ("O numero é divisivelpor 3 e 5")
 se numero % 3 != 0 & numero % 5 != 0
  imprime ("O numero não é divisivel")
   


#Execução

entrada ("Diga o seu numero: ", numero)

   se NumerosDivisiveis(numero):
    imprime("{numero} é divisivel por 3 e 5")
  senao:
   imprime("{numero}não é divisivel")
     